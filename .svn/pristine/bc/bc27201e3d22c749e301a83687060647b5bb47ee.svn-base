        #region GUARDAR DATOS

        private void Btn_guardar_Click(object sender, EventArgs e)
        {
            if (!txt_formato.Text.Equals(string.Empty) && !txt_nombre.Text.Equals(string.Empty) &&
                !txt_ancho.Text.Equals(string.Empty) && !txt_lineas.Text.Equals(string.Empty))
            {
                List<FormatorcDataSource> formato = new List<FormatorcDataSource>();
                formato = this.GuardarDatos();

                if (this.butonclick.Equals("agregar"))
                {
                    bool existe = this.negocioFormato.ExisteFormato(int.Parse(txt_formato.Text));
                    if (!existe)
                    {
                        bool guardo = this.negocioFormato.GuardoFormato(formato);
                        if (guardo)
                        {
                            MessageBox.Show("El concepto ha sido guardado", "GUARDAR", MessageBoxButtons.OK);
                            this.CamposInactivos();
                            this.CamposVacios();
                        }
                    }
                    else
                    {
                        MessageBox.Show("El formato ya existe", "ERROR", MessageBoxButtons.OK);
                    }
                }

                if (this.butonclick.Equals("editar"))
                {
                    bool guardo = this.negocioFormato.ActualizoFormato(formato);
                    if (guardo)
                    {
                        MessageBox.Show("El concepto ha sido actualizado", "GUARDAR", MessageBoxButtons.OK);
                        this.CamposInactivos();
                        this.CamposVacios();
                    }
                }
            }
            else
            {
                MessageBox.Show("Debe ingresar todos los datos", "ERROR", MessageBoxButtons.OK);
            }
        }

