        private void EnterValfor_KeyPress(object sender, KeyPressEventArgs e)
        {
            if (e.KeyChar == (char)Keys.Enter)
            {
                if (this.butonclick.Equals("agregar"))
                {
                    e.Handled = true;
                    SendKeys.Send("{TAB}");
                }
                else if (this.butonclick.Equals("editar"))
                {
                    e.Handled = true;
                    EventArgs eve = new EventArgs();
                    this.Btn_ir_Click(sender, eve);
                }
            }
            else
            {
                e.Handled = this.funtions.ValidacionNumerico(e).Handled;
            }
        }

